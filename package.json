{
  "name": "babel-plugin-contract",
  "version": "1.3.4",
  "description": "Design by Contract for JavaScript via a Babel plugin.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d ./lib ./src",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "mocha ./test/index.js",
    "watch": "NODE_WATCH=1 mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rockie/babel-plugin-contracts"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "design by contract",
    "types",
    "typing",
    "contracts",
    "type check",
    "dbyc"
  ],
  "author": "Rockie Guo <rockie@kitmi.com.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rockie/babel-plugin-contracts/issues"
  },
  "homepage": "https://github.com/rockie/babel-plugin-contracts",
  "dependencies": {
    "@babel/generator": "^7.9.0",
    "core-js": "^3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "mocha": "^7.1.1",
    "should": "^13.2.3"
  },
  "mocha": {
    "reporter": "spec",
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "should"
    ],
    "timeout": 30000
  }
}
